#include <iostream>
#include <cstring>

using namespace std;
const int cols = 16, rows = 15;

char words[rows][cols] = {"tgbwwinterwsesn",
                          "aaunttmmhfoodnb",
                          "jlwcqldzmpmvdmr",
                          "asagmquwvvbsohi",
                          "bwplotanadtpgnc",
                          "rewngodjcpnatnk",
                          "eeotwosbqharrsa",
                          "azcgeswewnaknpb",
                          "dinnerqodlwdcar",
                          "onopkwmparktzcc",
                          "qbfrogmamwpweey",
                          "lqzqnnmrzjjsclg",
                          "mosgzczetdbooto",
                          "pdcrzmsngrdnrpz",
                          "ohnkzwaterjgtra"};


char *getWordVertical(int);

char *reverse(char *);

bool searchVertical(char input[]){
	char *cek;
	char word[rows];
	for (int i=0;i<rows;i++){
		for (int j=0;j<rows;j++){
			word[j]=words[j][i];
		}
		cek=strstr(word, input);
		if(cek!=NULL){
			return true;
		}
	}
	return false;
}

bool searchHorizontal(char *);

char word[16];

int main()
{
    char word[16];
    int n;
    cin >> n;
    
    for (int i = 0; i < n; i++){
    	cin.ignore();
        cin.getline(word,16,'\n');
        
        if (searchVertical(word) || searchHorizontal(word)){
            cout << "Ada\n";
        }else{
            cout << "Tidak Ada\n";
		}
    }
    return 0;
}


bool searchHorizontal(char *wordin){
	wordin = word;
	
	for (int i = 0; i < 15; i++){
		char *periksa = strstr (words[i], wordin);
		
		if (periksa != 0){
			return true;
		}
	}
	
	return false;
}
